@model IEnumerable<GanapatiFeeds2.Models.Category>

@{
    ViewBag.Title = "Store";
}

<h2>Browse Category</h2>
<p> select from @Model.Count()
    genres:</p>

<ul>
    @foreach (var category in Model)
    {
        <li>@Html.ActionLink(category.Name,"Browse", new { category = category.Name })</li>
    }
</ul>

